<?xml version="1.0" encoding="UTF - 8"?>
<!--奇风UI窗口布局文件-->
<!--窗口布局主体必须QWMange（目前有且只能有一个），name随意设置-->
<QWMange name="" Rect="(5,5,500,400)">
    <!--每一个并行节点都是一个控件，控件节点元素名为：控件名，如按钮：QButton-->
    <!--控件属性有name：可以随意设置，尽量控件之间不要相同；Rect：控件矩形-->
    <QFSelect CurrentItem="0" FontColor="0xff9c0000" PopDialogHeight="180" PopDialogWidth="90" Rect="(64,269,200,56)" SelectChangeEvent="SelectChange" fontName="黑体" fontSize="16" name="QFSelect1">
        <Items>
            <Item fcolorD="0" fcolor1="0xff67ffff" fcolor2="0xffffff85" colorD="1" color1="0xffc0ff00" color2="0xff0084ce" value="TEST"/>
            <Item fcolorD="0" fcolor1="0xff67ffff" fcolor2="0xffffff85" colorD="1" color1="0xffc0ff00" color2="0xff0084ce" value="TEST1"/>
            <Item fcolorD="0" fcolor1="0xff67ffff" fcolor2="0xffffff85" colorD="1" color1="0xffc0ff00" color2="0xff0084ce" value="TEST2"/>
            <Item fcolorD="0" fcolor1="0xff67ffff" fcolor2="0xffffff85" colorD="1" color1="0xffc0ff00" color2="0xff0084ce" value="TEST3"/>
            <Item fcolorD="0" fcolor1="0xffffffff" fcolor2="0xffffffff" colorD="0" color1="0xff000000" color2="0xff000000" value="TEST4"/>
        </Items>
    </QFSelect>
    <QFSwitch IsMutex="false" Rect="(23,14,122,28)" State="true" StateChangeEvent="SwitchStateChange" name="QFSwitch1"/>
    <QFSlider IsSegmentation="true" RailColor="0xff00f0f0" RailRatio="0.321429" Rect="(25,62,436,35)" ValueChangeEvent="Sliding" backgroundColor="0xffffffff" currentValue="60" maxValue="100" minValue="0" name="QFSlider1">
        <Items>
            <SegmValue value="0.000000"/>
            <SegmValue value="20.000000"/>
            <SegmValue value="40.000000"/>
            <SegmValue value="60.000000"/>
            <SegmValue value="100.000000"/>
        </Items>
    </QFSlider>
    <QFProgress CurrentValue="56.632656" DecimalPlaces="0" Direction="0" FontColor="0xff00e100" FontSize="32" LineTypeColor="0xffff0000" Rect="(48,119,133,106)" RingProgressThickness="20" RingTypeColor1="0xff00a6d8" RingTypeColor2="0xffc9ff00" Type="1" name="QFProgress1"/>
    <QFColorBox ColorChangeEvent="ColorBoxChange" Rect="(415,178,213,36)" fontName="黑体" fontSize="20" name="QFColorBox1"/>
    <QFColorBox ColorChangeEvent="ColorBoxChange" fontName="黑体" fontSize="20" name="QFColorBox2" Rect="(414,216,213,36)"/>
</QWMange>
